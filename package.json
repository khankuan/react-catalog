{
  "name": "react-library-tool",
  "version": "1.0.0",
  "description": "React Library is a UI Component development and documentation tool",
  "repository": {
    "type": "git",
    "url": "khankuan/react-library"
  },
  "main": "src",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs": "babel-node ./scripts/index.js",
    "build-docs": "babel-node ./scripts/index.js --build",
    "publish-docs": "babel-node ./scripts/index.js --publish",
    "test-docs": "babel-node ./scripts/index.js --test",
    "lint-docs": "babel-node ./scripts/index.js --lint",
    "transpile-docs": "babel-node ./scripts/index.js --transpile",
    "build": "rm -rf dist && babel ./scripts --out-dir ./dist",
    "prepublish": "npm run build && npm run build-docs",
    "postpublish": "npm run publish-docs",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "react",
    "react-component",
    "documentation"
  ],
  "bin": {
    "react-library": "./bin/react-library.js"
  },
  "author": "khankuan <khankuan@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "classnames": "^2.2.5",
    "cross-spawn": "^4.0.0",
    "css-loader": "^0.26.1",
    "dom-helpers": "^3.2.0",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "fuse.js": "^2.4.1",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.5",
    "html-loader": "^0.4.3",
    "jest": "^17.0.0",
    "jest-cli": "^17.0.0",
    "js-beautify": "^1.6.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "markdown-loader": "^0.1.7",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-codemirror": "^0.2.6",
    "react-docgen": "2.12.1",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.1",
    "react-element-to-jsx-string": "^4.1.0",
    "react-markdown": "^2.4.2",
    "react-router": "^2.7.0",
    "react-test-renderer": "^15.4.0",
    "rimraf": "^2.5.4",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "uncontrollable": "^4.0.3",
    "watch": "^0.19.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "devDependencies": {
    "condition-circle": "^1.5.0",
    "semantic-release": "^6.3.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "release": {
    "verifyConditions": [
      "condition-circle"
    ],
    "debug": false
  }
}
